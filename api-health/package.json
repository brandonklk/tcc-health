{
  "name": "back-end-app",
  "version": "1.0.0",
  "description": "Aplicação do back-end",
  "main": "dist/src/server.js",
  "scripts": {
    "start": "cross-env TS_NODE_BASEURL=dist/src node -r tsconfig-paths/register dist/src/server.js",
    "build:db": "knex migrate:latest --env production --knexfile dist/src/database/knexfile.js && knex seed:run --env production --knexfile dist/src/database/knexfile.js",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist/src",
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/server.ts",
    "knex:migrate": "knex migrate:latest --knexfile src/database/knexfile.ts",
    "knex:rollback": "knex migrate:rollback --knexfile src/database/knexfile.ts",
    "knex:seeds": "knex seed:run --knexfile src/database/knexfile.ts",
    "test": "jest"
  },
  "author": "Brandon Marcos Kluck",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "celebrate": "^15.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.9",
    "multer": "^1.4.2",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.6.3",
    "pg": "^8.7.3",
    "pino": "^6.13.0",
    "pino-http": "^5.6.0",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2",
    "tsconfig-paths": "^4.0.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.2",
    "@types/hapi__joi": "^17.1.7",
    "@types/jest": "^27.5.1",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.34",
    "@types/pino": "^6.3.11",
    "@types/pino-http": "^5.4.2",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^28.0.3",
    "pino-pretty": "^5.1.2",
    "prettier": "^2.3.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.1",
    "ts-node": "^8.4.1",
    "ts-node-dev": "^1.1.8"
  }
}
